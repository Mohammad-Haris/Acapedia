{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "acapedia.io;www.acapedia.io",
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "QuotaExceededMessage": "",
    "DisableRateLimitHeaders": true,
    "GeneralRules": [
      {
        "Endpoint": "*:/Explore/GetUniversities",
        "Period": "1m",
        "Limit": 20
      },
      {
        "Endpoint": "*:/Explore/GetOnline",
        "Period": "1m",
        "Limit": 20
      }
    ]
  }
}
